{
  "name": "FastBtcV3",
  "private": true,
  "version": "2.0.0",
  "description": "Relay Btc deposits to RBtc",
  "scripts": {
    "start:test": "nodemon --config nodemon.json start.js testnet",
    "start:main": "nodemon --config nodemon.json start.js mainnet",
    "genAdminMainnet": "node -r esm utils/wordseed.js mainnet",
    "genAdminTestnet": "node -r esm utils/wordseed.js testnet",
    "withdrawBtcMainnet": "node -r esm utils/withdrawBtc.js mainnet",
    "withdrawBtcTestnet": "node -r esm utils/withdrawBtc.js testnet",
    "withdrawRskMainnet": "node -r esm utils/withdrawRsk.js mainnet",
    "withdrawRskTestnet": "node -r esm utils/withdrawRsk.js testnet",
    "build-client": "npx webpack --config ./webpack.config.js && cp src/index.html dist/index.html",
    "watch-client": "webpack --watch --config ./webpack.config.js",
    "test-rsk": "mocha -r esm tests/test_rskCtrl.js --timeout 2500000"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@node-rpc/client": "^2.3.1",
    "async-mutex": "^0.2.6",
    "axios": "^0.21.1",
    "bip39": "^3.0.2",
    "bitcoinjs-lib": "^5.2.0",
    "bitcore-lib": "^8.23.1",
    "bs58": "^4.0.1",
    "convert-hex": "^0.1.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "electrum-mnemonic": "^2.0.0",
    "esm": "^3.2.25",
    "ethereumjs-util": "^7.0.7",
    "ethereumjs-wallet": "^1.0.1",
    "hdkey": "^2.0.1",
    "js-sha256": "^0.9.0",
    "json2csv": "^5.0.5",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "pm2-server-monitor": "^1.0.0",
    "qr-encode": "^0.3.0",
    "rpc-client": "^1.0.1",
    "socket.io": "^3.1.1",
    "sqlite3": "^5.0.0",
    "telegraf": "^3.38.0",
    "web3": "^1.2.11"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.1.1",
    "nodemon": "^2.0.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
